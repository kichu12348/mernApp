{
  "expo": {
    "name": "quiply",
    "slug": "quiply",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kichu12348.quiply",
      "config":{
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kichu12348.quiply"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "16627948-5b49-452f-a6f2-5ebf2015b012"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-updates",
        {
          "username": "account-username"
        }
      ]
    ]
  }
}
